{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev --turbopack",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.3",
    "i18next": "^23.16.8",
    "next": "16.0.0",
    "next-i18next": "^15.4.2",
    "next-language-detector": "^1.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "^15.7.4",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@mui/icons-material": "^7.3.4",
    "@next/bundle-analyzer": "16.0.0",
    "@next/eslint-plugin-next": "16.0.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@types/react": "19.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.11.6",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.0",
    "styled-jsx": "^5.1.7",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}

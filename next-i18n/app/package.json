{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev --turbopack",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint -c ./eslint.config.mjs './**/*.{ts,tsx}'",
    "lint-fix": "eslint -c ./eslint.config.mjs './**/*.{ts,tsx}' --fix",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@mui/material-nextjs": "^7.0.2",
    "i18next": "^23.16.8",
    "next": "^15.3.1",
    "next-i18next": "^15.4.2",
    "next-language-detector": "^1.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@mui/icons-material": "^7.0.2",
    "@next/bundle-analyzer": "^15.3.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@stylistic/eslint-plugin-js": "^2.13.0",
    "@stylistic/eslint-plugin-jsx": "^2.13.0",
    "@stylistic/eslint-plugin-plus": "^2.13.0",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@swc/jest": "^0.2.38",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^14.2.28",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.11.6",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "styled-jsx": "^5.1.6",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}

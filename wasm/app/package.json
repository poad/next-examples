{
  "name": "wasm-example",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "cargo install wasm-pack",
    "dev": "pnpm wasm && next dev --turbopack",
    "build": "pnpm wasm && next build --webpack",
    "start": "pnpm wasm && next start",
    "analyze": "ANALYZE=true next build",
    "export": "pnpm wasm && next build",
    "wasm": "wasm-pack build --target web --no-opt wasm",
    "lint": "eslint",
    "lint-fix": "eslint --fix"
  },
  "dependencies": {
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.1.4",
    "@next/eslint-plugin-next": "^16.1.4",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  }
}
